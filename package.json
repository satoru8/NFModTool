{
  "name": "nfmodtool",
  "productName": "NF Mod Tool",
  "icon": "./logo.png",
  "version": "0.0.1",
  "description": "Modding Tool for Noble Fates",
  "main": "./dist/main/index.js",
  "repository": "https://gitlab.com/satoru8/nfmodtool",
  "homepage": "https://gitlab.com/satoru8/nfmodtool",
  "scripts": {
    "cleaninstall": "rmdir /s /q node_modules || echo Folder node_modules not found & rmdir /s /q dist || echo Folder dist not found & del package-lock.json || echo File package-lock.json not found & npm i & npm run start",
    "update all": "npm update",
    "start": "electron-vite preview --outDir=dist",
    "dev": "electron-vite dev --outDir=dist -w",
    "package": "electron-vite build --outDir=dist && electron-forge package",
    "make ": "electron-vite build --outDir=dist && electron-forge make",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "prettier": "prettier --write src/"
  },
  "keywords": [
    "electron",
    "vite",
    "vue",
    "modding",
    "noble fates"
  ],
  "author": {
    "name": "Sato",
    "email": "katsuryoku2@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "@electron-forge/plugin-vite": "^7.3.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "antlr4": "^4.13.1-patch-1",
    "electron": "^29.1.4",
    "electron-vite": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "monaco-editor": "^0.46.0",
    "prettier": "^3.2.5",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue3-tree": "^0.11.5"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.19",
    "vuetify": "^3.5.7"
  }
}
